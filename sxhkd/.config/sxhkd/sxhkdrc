    ~/bin/reset-minivault

super + m
 polybar-msg cmd toggle


# lock screen
super + w
    ~/bin/lock

# demnu to lock/restart/shutdown/etcc
super + control + r
  ~/bin/computer

super + x
  rofi -show window

# taking screnshot
super + shift + w
    ~/bin/dscreenshot
# dmenu with chrome bookmarks
super + comma
    ~/bin/chrome-bookmarks
# dmenu link handler
super + period
    ~/bin/dmenu-links
# dmenu to copy docker container id
super + shift + d
    ~/bin/dmenu_docker

# increase sound
super + equal
    pulsemixer --change-volume +5
# decrease sound
super + plus
    pulsemixer --change-volume -5
# mute sound
XF86AudioMute
    pulsemixer --toggle-mute
# mpc toggle play/pause
super + s
    mpc toggle

# dmenu to launch app
super + r
  dmenu_run -l 20
